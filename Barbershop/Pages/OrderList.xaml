<Page x:Class="Barbershop.Pages.OrderList"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Barbershop.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="OrderList">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/Pages/Background.png"/>
        </Grid.Background>
        <DataGrid x:Name="dataGrid" Margin="87,10,87,110" SelectionChanged="dataGrid_SelectionChanged" IsReadOnly="True"/>
        <Button x:Name="button" Content="Назад" HorizontalAlignment="Center" Margin="0,359,0,0" VerticalAlignment="Top" Click="button_Click"/>

    </Grid>
</Page>
